# =============================================================================
# FRONTEND PRODUCTION ENVIRONMENT CONFIGURATION
# =============================================================================
# IMPORTANT: Replace all placeholder values with real production values

# =============================================================================
# API Configuration (Production)
# =============================================================================
REACT_APP_API_URL=https://www.mokhtarelhenawy.online/api/v1
REACT_APP_WS_URL=wss://mokhtarelhenawy.online

# =============================================================================
# Facebook SDK (Production)
# =============================================================================
REACT_APP_FACEBOOK_APP_ID=your-production-facebook-app-id

# =============================================================================
# Google Services (Production)
# =============================================================================
REACT_APP_GOOGLE_MAPS_API_KEY=your-production-google-maps-api-key

# =============================================================================
# Payment Gateways (Production)
# =============================================================================
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_your-production-stripe-publishable-key
REACT_APP_PAYPAL_CLIENT_ID=your-production-paypal-client-id

# =============================================================================
# App Configuration (Production)
# =============================================================================
REACT_APP_APP_NAME="Communication Platform"
REACT_APP_APP_VERSION=1.0.0
REACT_APP_COMPANY_NAME="Mokhtar Elhenawy"
REACT_APP_BACKEND_URL=https://www.mokhtarelhenawy.online

# =============================================================================
# Feature Flags (Production)
# =============================================================================
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_ECOMMERCE=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# =============================================================================
# File Upload (Production)
# =============================================================================
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# =============================================================================
# Theme Configuration (Production)
# =============================================================================
REACT_APP_DEFAULT_THEME=light
REACT_APP_PRIMARY_COLOR=#3B82F6
REACT_APP_SECONDARY_COLOR=#10B981

# =============================================================================
# Analytics (Production)
# =============================================================================
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your-production-mixpanel-token

# =============================================================================
# Error Tracking (Production)
# =============================================================================
REACT_APP_SENTRY_DSN=your-production-sentry-dsn

# =============================================================================
# Development Settings (Production - Disabled)
# =============================================================================
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=error

# =============================================================================
# PWA Configuration (Production)
# =============================================================================
REACT_APP_ENABLE_PWA=true
REACT_APP_PWA_NAME="Communication Platform"
REACT_APP_PWA_SHORT_NAME="CommPlatform"

# =============================================================================
# Localization (Production)
# =============================================================================
REACT_APP_DEFAULT_LANGUAGE=ar
REACT_APP_SUPPORTED_LANGUAGES=ar,en

# =============================================================================
# Real-time Features (Production)
# =============================================================================
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_HEARTBEAT_INTERVAL=30000

# =============================================================================
# Cache Configuration (Production)
# =============================================================================
REACT_APP_CACHE_DURATION=300000

# =============================================================================
# WhatsApp Integration (Production)
# =============================================================================
REACT_APP_WHATSAPP_BUSINESS_PHONE=your-production-whatsapp-phone

# =============================================================================
# WooCommerce Integration (Production)
# =============================================================================
REACT_APP_WOOCOMMERCE_STORE_URL=https://your-woocommerce-store.com

# =============================================================================
# Security Settings (Production)
# =============================================================================
REACT_APP_ENVIRONMENT=production
REACT_APP_SECURE_MODE=true
